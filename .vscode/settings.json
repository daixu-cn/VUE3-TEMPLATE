{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": [
    "autoresize",
    "boxplot",
    "cascader",
    "commitlint",
    "datetime",
    "datetimerange",
    "echarts",
    "formkit",
    "headerbtn",
    "ICONFONT",
    "iconify",
    "nightwatch",
    "nprogress",
    "offcanvas",
    "Parens",
    "persistedstate",
    "pinia",
    "radash",
    "resizability",
    "ress",
    "rgba",
    "rushstack",
    "sankey",
    "screenreaders",
    "scrollbars",
    "tsbuildinfo",
    "unplugin",
    "vuetify",
    "vueuse"
  ],
  "i18n-ally.localesPaths": ["src/locale/languages"],
  "i18n-ally.enabledParsers": ["json"],
  "i18n-ally.sourceLanguage": "en-US",
  "i18n-ally.extract.keygenStyle": "PascalCase",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.translate.engines": ["deepl", "google", "google-cn", "baidu"]
}
