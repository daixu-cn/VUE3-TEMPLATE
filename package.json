{
  "name": "vue-template",
  "type": "module",
  "author": "daixu <wywangt@outlook.com> (https://daixu.cn/)",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "prettier": "prettier --write .",
    "commit": "git-cz",
    "husky-install": "npx husky init && echo \"npm run prettier && npm run lint\" > .husky/pre-commit && echo \"npx --no -- commitlint --edit \\$1\" > .husky/commit-msg",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@formkit/auto-animate": "^0.8.2",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.9.7",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "radash": "^12.1.0",
    "vue": "^3.5.13",
    "vue-echarts": "^7.0.3",
    "vue-i18n": "^11.1.2",
    "vue-iconify": "^1.0.11",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/cz-commitlint": "^19.8.0",
    "@commitlint/prompt-cli": "^19.8.0",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^22.13.14",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-legacy": "^6.0.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cssnano": "^7.0.6",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "^9.1.7",
    "npm-run-all2": "^7.0.2",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass-embedded": "^1.86.0",
    "typescript": "^5.8.2",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-element-plus": "^0.9.1",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "2.2.8"
  }
}
